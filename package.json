{
  "name": "puppeteer-request-spy",
  "version": "1.0.2",
  "description": "simple request spy helper for testing with puppeteer",
  "main": "build/src/index.js",
  "scripts": {
    "build": "node_modules/.bin/tsc -p tsconfig.json",
    "pretest": "npm run build",
    "test": "node_modules/.bin/mocha --require source-map-support/register build/test/**/*.spec.js",
    "test-silent": "node_modules/.bin/mocha --require source-map-support/register build/test/**/*.spec.js > test-ts.log",
    "pretest-coverage": "node_modules/.bin/tsc  -p tsconfig.json --sourcemap",
    "test-coverage": "node_modules/.bin/nyc --all --reporter=html npm run test-silent"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "build/src/**/*.js"
    ],
    "exclude": [
      "**/Logger.js",
      "**/index.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "keywords": [
    "puppeteer",
    "request",
    "spy",
    "testing"
  ],
  "types": "types/index.d.ts",
  "author": "Tobias Nie√üen",
  "license": "MIT",
  "devDependencies": {
    "@types/puppeteer": "^0.13.7",
    "minimatch": "^3.0.4",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "puppeteer": "^0.13.0",
    "sinon": "^4.1.3",
    "source-map-support": "^0.5.0",
    "static-server": "^3.0.0",
    "typescript": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tabueeee/puppeteer-request-spy.git"
  }
}
